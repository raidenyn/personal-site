{
  "name": "raidenyn-home",
  "description": "My Home Page",
  "version": "0.0.0",
  "author": "Yuriy Nagaev <yuriy@nagaev.me>",
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3",
    "yarn": ">=1.2"
  },
  "files": [
    "dist",
    "helpers",
    "src/types",
    "server.*.ts",
    "tsconfig.node.json",
    "yarn.lock"
  ],
  "scripts": {
    "build": "yarn run test && yarn run prod && yarn run package",
    "clean": "rimraf dist && rimraf coverage && rimraf dist.tgz",
    "package": "yarn pack --filename dist.tgz",
    "prod": "yarn run clean && run-p prod:client prod:sw prod:server",
    "prod:client": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.node.json webpack --config config/webpack.config.prod.ts --progress --hide-modules --mode production",
    "prod:server": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.node.json webpack --config config/webpack.config.server.ts --progress --hide-modules --mode production",
    "prod:sw": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.node.json webpack --config config/webpack.config.sw.ts --progress --hide-modules --mode production",
    "prod:ssr": "cross-env NODE_ENV=production ts-node --project=tsconfig.node.json server.prod.ts",
    "coverage": "yarn run coverage:run && yarn run coverage:remap && yarn run coverage:open",
    "coverage:open": "opn coverage/html-ts/index.html",
    "coverage:remap": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html-ts -t html",
    "coverage:run": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.node.json karma start karma.coverage.ts",
    "dev": "yarn run clean && run-p dev:ssr dev:client dev:sw dev:server",
    "dev:client": "cross-env TS_NODE_PROJECT=tsconfig.node.json webpack-dev-server -d --hot --config config/webpack.config.dev.ts --mode development",
    "dev:server": "cross-env TS_NODE_PROJECT=tsconfig.node.json webpack --watch --config config/webpack.config.server.ts --mode development",
    "dev:sw": "cross-env TS_NODE_PROJECT=tsconfig.node.json webpack --watch --config config/webpack.config.sw.ts --mode development",
    "dev:ssr": "nodemon --watch server.*.ts --exec ts-node --project=tsconfig.node.json server.dev.ts",
    "test": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.node.json karma start karma.unit.ts",
    "test:debug": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.node.json karma start karma.debug.ts",
    "test:watch": "cross-env NODE_ENV=development TS_NODE_PROJECT=tsconfig.node.json karma start karma.unit.ts --singleRun=false --auto-watch"
  },
