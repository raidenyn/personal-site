{
    "compilerOptions": {
        "lib": [
            "webworker",
            "es2017",
        ],
        "module": "esnext",
        "moduleResolution": "node",
        "target": "es2017",
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "alwaysStrict": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "noEmitHelpers": true,
        "importHelpers": true,
        "noImplicitReturns": true
    },
}
